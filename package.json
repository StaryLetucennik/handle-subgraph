{
  "name": "handle-subgraph",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/handle-fi/handle-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create handle-fi/handle --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy handle-fi/handle --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy": "graph deploy handle-fi/handle --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-staging": "graph deploy handle-fi/handle /handle --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "watch-local": "graph deploy handle-fi/handle --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@types/node": "^14.14.41",
    "typescript": "^4.2.4"
  }
}
